<%= form_with model: praesidium do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
    <% praesidium.errors.full_messages_for(:name).each do |message| %>

    <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :parish %><br>
    <%= form.text_field :parish %>
    <% praesidium.errors.full_messages_for(:location).each do |message| %>

    <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :date_created %><br>
    <%= form.date_field :date_created %>
    <% praesidium.errors.full_messages_for(:date_created).each do |message| %>
    <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :curia %><br>
    <%= form.collection_select(:curia_id, Curia.all, :id, :name) %>
    <% praesidium.errors.full_messages_for(:curia_id).each do |message| %>
    <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
